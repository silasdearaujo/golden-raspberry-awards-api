datasource db {
  provider = "sqlite"
  url      = "file:./dev.db?mode=memory&cache=shared"
}

generator client {
  provider = "prisma-client-js"
}

model Movie {
  id        String  @id @default(nanoid())
  year      Int
  title     String
  studios   String
  winner    Boolean @default(false)
  producers String
}
